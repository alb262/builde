@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lite: <http://bibfra.me/vocab/lite/> .
@prefix rel: <http://bibfra.me/vocab/relation/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix lcrel: <http://id.loc.gov/vocabulary/relators/> .
@prefix lcroles: <http://id.loc.gov/entities/roles/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix oa: <http://www.w3.org/ns/oa#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .

# BIBFRAME Linked Data Extension aka BUILDE Relation üìñüîç

# This RDF vocabulary encompasses a wide range of classes and properties suitable for describing various
# types of bibliographic resources, scholarly works, and related entities. BUILDE includes a basic set of 
# core classes and properties known as the 'lite' layer described here, as well as several extensions:

#	archive: things found in archives
#	rare books: things found in rare book libraries
#	scholar: things found in scholarly research and publishing
#	library: things found in libraries

# This vocabulary is ideal for librarians, information scientists, data curators, and developers working
# in academic and research settings. It can be employed in creating digital libraries, scholarly databases,
# cataloging systems, and other applications requiring structured bibliographic information.

# Key Features:
# - Compatibility with existing ontologies including Schema.org, SKOS, FOAF, and Dublin Core for enhanced interoperability.
# - Extension of BIBFRAME from the Library of Congress
# - Comprehensive coverage of bibliographic and scholarly domains.
# - Clear, self-explanatory labels and definitions for each class and property.
# - Hierarchical organization of classes using rdfs:subClassOf for structured representation.

# We encourage users to provide feedback and suggestions to continually improve and update this vocabulary.
# For versioning, updates, and more detailed documentation, please visit [https://bibfra.me/?].

rel: a rdfs:Vocabulary ;
    rdfs:label "Relation" ;
    rdfs:comment "Builde vocabulary for relationships between resources" ;
    dc:creator "EBSCO Information Systems"^^xsd:date ;
    dc:created "2024-07-22"^^xsd:date ;
    dc:issued "2024-08-22"^^xsd:date ;
    dc:modified "2024-08-22"^^xsd:date ;
    owl:versionInfo "1.0" .

## Relationships between Works

rel:absorbed a rdf:Property ;
    rdfs:label "absorbed"@en ;
    rdfs:comment "Work that has been incorporated into another Work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:absorbedBy a rdf:Property ;
    rdfs:label "absorbed by"@en ;
    rdfs:comment "Resource was absorbed by another resource."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:absorbedInPart a rdf:Property ;
    rdfs:label "absorbed in part"@en ;
    rdfs:comment "Work that has been partially incorporated into another work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:absorbedInPartBy a rdf:Property ;
    rdfs:label "absorbed in part by"@en ;
    rdfs:comment "Resource was partially absorbed by another resource."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:changedBackTo a rdf:Property ;
    rdfs:label "changed back to"@en ;
    rdfs:comment "Work that has been changed back into another work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:continueBy a rdf:Property ;
    rdfs:label "continue by"@en ;
    rdfs:comment "Work whose content continues an earlier work under a new title."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:continues a rdf:Property ;
    rdfs:label "continues"@en ;
    rdfs:comment "Work that is continued by the content of a later work under a new title."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:continuesInPart a rdf:Property ;
    rdfs:label "continues in part"@en ;
    rdfs:comment "Work that split into two or more separate works with new titles."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:continuedInPartBy a rdf:Property ;
    rdfs:label "continued in part by"@en ;
    rdfs:comment "Content of a previous resource is partially continued in a new resource."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasBasis a rdf:Property ;
    rdfs:label "has basis"@en ;
    rdfs:comment "Trarget resource is the basis of the source resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasDerivative a rdf:Property ;
    rdfs:label "has derivative"@en ;
    rdfs:comment "Target resource is a modification of the source resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasEdition a rdf:Property ;
    rdfs:label "has edition"@en ;
    rdfs:comment "Target resource is a particular version of a source resource that has been revised or created."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasExpression a rdf:Property ;
    rdfs:label "has expression"@en ;
    rdfs:comment "Resource has a related expression. For use to connect Works under FRBR/RDA rules."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasEquivalent a rdf:Property ;
    rdfs:label "has equivalent"@en ;
    rdfs:comment "Embodies the same as the resource being described."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasPart a rdf:Property ;
    rdfs:label "has part"@en ;
    rdfs:comment "Resource that is included either physically or logically contained in the described resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:isAdaptationOf a rdf:Property ;
    rdfs:label "is adaptation of"@en ;
    rdfs:comment "Source resource is an adaptation of the target resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:isBasedOn a rdf:Property ;
    rdfs:label "is based on"@en ;
    rdfs:comment "Source resource is based on the target resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    owl:sameAs schema:isBasedOn ;
    rdfs:subPropertyOf lite:related .

rel:isDerivativeOf a rdf:Property ;
    rdfs:label "is derivative of"@en ;
    rdfs:comment "Source resource is a modification of the target resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:isEditionOf a rdf:Property ;
    rdfs:label "is edition of"@en ;
    rdfs:comment "Source resource is a particular version of a target resource that has been revised or created."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:isPartOf a rdf:Property ;
    rdfs:label "is part of"@en ;
    rdfs:comment "Relationship that connects things that are part of other things."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:isTranslationOf a rdf:Property ;
    rdfs:label "is translation of"@en ;
    rdfs:comment "Work that has been translated, i.e., the text expressed in a language different from that of the original work."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasAdaptation a rdf:Property ;
    rdfs:label "has adaptation"@en ;
    rdfs:comment "Target resource is an adaptation of the source resource."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:hasTranslation a rdf:Property ;
    rdfs:label "has translation"@en ;
    rdfs:comment "Work that translates the text of the source entity into a language different from that of the original."@en ;
    rdfs:range lite:Resource ;
    rdfs:domain lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:mergesWith a rdf:Property ;
    rdfs:label "merges with"@en ;
    rdfs:comment "Two or more resources combine to create a single resource."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:mergedWith a rdf:Property ;
    rdfs:label "merged with"@en ;
    rdfs:comment "One of two or more works that come together to form a new work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:separatedFrom a rdf:Property ;
    rdfs:label "separated from"@en ;
    rdfs:comment "Work that spun off a part of its content to form a new work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:splitInto a rdf:Property ;
    rdfs:label "split into"@en ;
    rdfs:comment "One of two or more works resulting from the division of an earlier work into separate works."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:supersedes a rdf:Property ;
    rdfs:label "supersedes"@en ;
    rdfs:comment "Earlier work whose content has been replaced by a later work, usually because the later work contains updated or new information."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:supersededBy a rdf:Property ;
    rdfs:label "superseded by"@en ;
    rdfs:comment "Later Work used in place of an earlier work, usually because the later work contains updated or new information."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:supersedesInPart a rdf:Property ;
    rdfs:label "supersedes in part"@en ;
    rdfs:comment "Earlier work whose content has been partially replaced by a later work, usually because the later work contains updated or new information."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:supersededInPartBy a rdf:Property ;
    rdfs:label "superseded in part by"@en ;
    rdfs:comment "Resource has been partially superseded by another resource."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

rel:unionOf a rdf:Property ;
    rdfs:label "union of"@en ;
    rdfs:comment "One of two or more works which came together to form a new work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:related .

## Relationships derived from MARC relator terms

rel:abridger a rdf:Property ;
    rdfs:label "abridger"@en ;
    rdfs:comment "Professional contributing to a resource by shortening or condensing the original work but leaving the nature and content of the original work substantially unchanged."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:abr ;
    rdfs:subPropertyOf lite:contributor .

rel:actor a rdf:Property ;
    rdfs:label "actor"@en ;
    rdfs:comment "Performer contributing to an expression of a work by acting as a cast member or player in a musical or dramatic presentation, etc."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:act ;
    rdfs:subPropertyOf lite:contributor .

rel:adapter a rdf:Property ;
    rdfs:label "adapter"@en ;
    rdfs:comment "Person or organization who 1) reworks a musical composition, usually for a different medium, or 2) rewrites novels or stories for motion pictures or other audiovisual medium."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:adp ;
    rdfs:subPropertyOf lite:contributor .

rel:addressee a rdf:Property ;
    rdfs:label "addressee"@en ;
    rdfs:comment "Person, family, or organization to whom the correspondence in a work is addressed."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:rcp ;
    rdfs:subPropertyOf lite:contributor .

rel:analyst a rdf:Property ;
    rdfs:label "analyst"@en ;
    rdfs:comment "Person or organization that reviews, examines and interprets data or information in a specific area."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:anl ;
    rdfs:subPropertyOf lite:contributor .

rel:animator a rdf:Property ;
    rdfs:label "animator"@en ;
    rdfs:comment "Person contributing to a moving image work or computer program by giving apparent movement to inanimate objects or drawings."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:anm ;
    rdfs:subPropertyOf lite:contributor .

rel:annotator a rdf:Property ;
    rdfs:label "annotator"@en ;
    rdfs:comment "Person who makes manuscript annotations on an item."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:ann ;
    rdfs:subPropertyOf lite:contributor .

rel:appellant a rdf:Property ;
    rdfs:label "appellant"@en ;
    rdfs:comment "Person or organization who appeals a lower court's decision."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:apl ;
    rdfs:subPropertyOf lite:contributor .

rel:appellee a rdf:Property ;
    rdfs:label "appellee"@en ;
    rdfs:comment "Person or organization against whom an appeal is taken."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:ape ;
    rdfs:subPropertyOf lite:contributor .

rel:applicant a rdf:Property ;
    rdfs:label "applicant"@en ;
    rdfs:comment "Person or organization responsible for the submission of an application or who is named as eligible for the results of the processing of the application (e.g., bestowing of rights, reward, title, position)."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:app ;
    rdfs:subPropertyOf lite:contributor .

rel:appraiser a rdf:Property ;
    rdfs:label "appraiser"@en ;
    rdfs:comment "Professional who evaluates and determines the value of property, assets, or goods, often for purposes such as insurance, taxation, or sale."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:exp ;
    rdfs:subPropertyOf lite:contributor .

rel:architect a rdf:Property ;
    rdfs:label "architect"@en ;
    rdfs:comment "Professional responsible for creating an architectural design, including a pictorial representation intended to show how a building, etc., will look when completed. Architects also oversees the construction of structures."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:arc ;
    rdfs:subPropertyOf lite:contributor .

rel:arranger a rdf:Property ;
    rdfs:label "arranger"@en ;
    rdfs:comment "Professional contributing to a musical work by rewriting the composition for a medium of performance different from that for which the work was originally intended, or modifying the work for the same medium of performance, etc., such that the musical substance of the original composition remains essentially unchanged. For extensive modification that effectively results in the creation of a new musical work, see composer."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:arr ;
    rdfs:subPropertyOf lite:contributor .

rel:arrangerOfMusic a rdf:Property ;
    rdfs:label "arranger of music"@en ;
    rdfs:comment "Professional musician who adapts and organizes musical compositions, modifying elements like instrumentation, harmony, or structure to suit a particular performance or style."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcroles:arrangerofmusic ;
    rdfs:subPropertyOf lite:contributor .

rel:artCopyist a rdf:Property ;
    rdfs:label "art copyist"@en ;
    rdfs:comment "Professional (e.g., a painter or sculptor) who makes copies of works of visual art."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:acp ;
    rdfs:subPropertyOf lite:contributor .

rel:artDirector a rdf:Property ;
    rdfs:label "art director"@en ;
    rdfs:comment "Professional contributing to a motion picture or television production by overseeing the artists and craftspeople who build the sets."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:adi ;
    rdfs:subPropertyOf lite:contributor .

rel:artist a rdf:Property ;
    rdfs:label "artist"@en ;
    rdfs:comment "Professional responsible for creating a work by conceiving, and implementing, an original graphic design, drawing, painting, etc."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:art ;
    rdfs:subPropertyOf lite:contributor .

rel:artisticDirector a rdf:Property ;
    rdfs:label "artistic director"@en ;
    rdfs:comment "Professional responsible for controlling the development of the artistic style of an entire production, including the choice of works to be presented and selection of senior production staff."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:ard ;
    rdfs:subPropertyOf lite:contributor .

rel:assignee a rdf:Property ;
    rdfs:label "assignee"@en ;
    rdfs:comment "Person or organization to whom a license for printing or publishing has been transferred."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:asg ;
    rdfs:subPropertyOf lite:contributor .

rel:associatedName a rdf:Property ;
    rdfs:label "associated name"@en ;
    rdfs:comment "Person or organization associated with or found in an item or collection, which cannot be determined to be that of a former owner or other designated relationship indicative of provenance."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:asn ;
    rdfs:subPropertyOf lite:contributor .

rel:attributedName a rdf:Property ;
    rdfs:label "attributed name"@en ;
    rdfs:comment "An author, artist, etc., relating him/her to a resource for which there is or once was substantial authority for designating that person as author, creator, etc. of the work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:att ;
    rdfs:subPropertyOf lite:contributor .

rel:auctioneer a rdf:Property ;
    rdfs:label "auctioneer"@en ;
    rdfs:comment "Person or organization in charge of the estimation and public auctioning of goods, particularly books, artistic works, etc."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:auc ;
    rdfs:subPropertyOf lite:contributor .

rel:author a rdf:Property ;
    rdfs:label "author"@en ;
    rdfs:comment "Person, family, or organization responsible for creating a work that is primarily textual in content, regardless of media type (e.g., printed text, spoken word, electronic text, tactile text) or genre (e.g., poems, novels, screenplays, blogs). Use also for persons, etc., creating a new work by paraphrasing, rewriting, or adapting works by another creator such that the modification has substantially changed the nature and content of the original or changed the medium of expression."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:aut ;
    rdfs:subPropertyOf lite:contributor .

rel:authorInQuotationsOrTextAbstracts a rdf:Property ;
    rdfs:label "author in quotations or text abstracts"@en ;
    rdfs:comment "Person or organization whose work is largely quoted or extracted in works to which he or she did not contribute directly. Such quotations are found particularly in exhibition catalogs, collections of photographs, etc."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:aqt ;
    rdfs:subPropertyOf lite:contributor .

rel:authorOfAfterwordColophonEtc a rdf:Property ;
    rdfs:label "author of afterword, colophon, etc."@en ;
    rdfs:comment "Person or organization responsible for an afterword, postface, colophon, etc. but who is not the chief author of a work."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:aft ;
    rdfs:subPropertyOf lite:contributor .

rel:authorOfDialog a rdf:Property ;
    rdfs:label "author of dialog"@en ;
    rdfs:comment "Person or organization responsible for the dialog or spoken commentary for a screenplay or sound recording."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:aud ;
    rdfs:subPropertyOf lite:contributor .

rel:authorOfIntroductionEtc a rdf:Property ;
    rdfs:label "author of introduction"@en ;
    rdfs:comment "Person or organization responsible for an introduction, preface, foreword, or other critical introductory matter, but who is not the chief author."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:aui ;
    rdfs:subPropertyOf lite:contributor .

rel:authorOfScreenplayEtc a rdf:Property ;
    rdfs:label "author of screenplay"@en ;
    rdfs:comment "Person or organization responsible for writing a screenplay, script, or other narrative material for a performance or production."@en ; 
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    rdfs:subPropertyOf lite:contributor .

rel:autographer a rdf:Property ;
    rdfs:label "autographer"@en ;
    rdfs:comment "Person whose manuscript signature appears on an item."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:ato ;
    rdfs:subPropertyOf lite:contributor .

rel:bibliographicAntecedent a rdf:Property ;
    rdfs:label "bibliographic antecedent"@en ;
    rdfs:comment "Person or organization responsible for a resource upon which the resource represented by the bibliographic description is based. This may be appropriate for adaptations, sequels, continuations, indexes, etc."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:ant ;
    rdfs:subPropertyOf lite:contributor .

rel:binder a rdf:Property ;
    rdfs:label "binder"@en ;
    rdfs:comment "Person who binds an item."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:bnd ;
    rdfs:subPropertyOf lite:contributor .

rel:bindingDesigner a rdf:Property ;
    rdfs:label "binding designer"@en ;
    rdfs:comment "Person or organization responsible for the binding design of a book, including the type of binding, the type of materials used, and any decorative aspects of the binding."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:bdd ;
    rdfs:subPropertyOf lite:contributor .

rel:blurbWriter a rdf:Property ;
    rdfs:label "blurb writer"@en ;
    rdfs:comment "Person or organization responsible for writing a commendation or testimonial for a work, which appears on or within the publication itself, frequently on the back or dust jacket of print publications or on advertising material for all media."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:blw ;
    rdfs:subPropertyOf lite:contributor .

rel:bookDesigner a rdf:Property ;
    rdfs:label "book designer"@en ;
    rdfs:comment "Person or organization involved in manufacturing a manifestation by being responsible for the entire graphic design of a book, including arrangement of type and illustration, choice of materials, and process used."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:bkd ;
    rdfs:subPropertyOf lite:contributor .

rel:bookProducer a rdf:Property ;
    rdfs:label "book producer"@en ;
    rdfs:comment "Person or organization responsible for the production of books and other print media."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:bkp ;
    rdfs:subPropertyOf lite:contributor .

rel:bookJacketDesigner a rdf:Property ;
    rdfs:label "bookjacket designer"@en ;
    rdfs:comment "Person or organization responsible for the design of flexible covers designed for or published with a book, including the type of materials used, and any decorative aspects of the bookjacket."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
    owl:sameAs lcrel:bjd ;
    rdfs:subPropertyOf lite:contributor .

rel:bookplateDesigner a rdf:Property ;
    rdfs:label "bookplate designer"@en ;
    rdfs:comment "Person or organization responsible for the design of a book owner's identification label that is most commonly pasted to the inside front cover of a book."@en ;
    rdfs:domain lite:Resource ;
    rdfs:range lite:Resource ;
